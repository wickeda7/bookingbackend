    },
    "user_favorites": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "favorites"
    },
    "timeslot": {
      "type": "json"
    },
    "email": {
      "type": "string"
